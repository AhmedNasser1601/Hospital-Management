@model IEnumerable<User>

@{
    ViewData["Title"] = "Users";
}

<div class="row">
    <div class="col-md-8 offset-2">
        <div class="row">
            <div class="col-md-8 offset-2">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>UserName</th>
                            <th>E-mail</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var Model in Model)
                        {
                            <tr>
                                <td>@Model.Name</td>
                                <td>@Model.UserName</td>
                                <td>@Model.Email</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
